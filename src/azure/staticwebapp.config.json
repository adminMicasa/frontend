{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/api"
        ]
    },
    "platform": {
        "build": "dist",
        "publish": "dist"
    }
}